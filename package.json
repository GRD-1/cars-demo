{
  "name": "cars-demo",
  "version": "1.0.0",
  "description": "",
  "module": "app/index.js",
  "keywords": [
    "REST",
    "TypeScript",
    "Express",
    "JWT",
    "CLI"
  ],
  "author": "GRD",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "start:prod": "npm run build && dotenvx run --env-file=env/prod.env -- node dist/index.js",
    "start:dev": "tsc-watch --onSuccess 'nodemon --config devops/nodemon-dev.json src/index.ts'",
    "start:debug": "tsc-watch --onSuccess 'nodemon --config devops/nodemon-debug.json src/index.ts'",
    "run-typescript": "ts-node-transpile-only",
    "build": "tsc",
    "test:unit": "jest --config src/test/unit/jest.unit-config.json",
    "test:int": "jest --config src/test/integration/jest.integration-config.json",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage --config jest.config.json",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^0.14.1",
    "body-parser": "^1.20.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.3.0",
    "express": "^4.18.2",
    "mongodb": "^5.9.2",
    "mongoose": "^8.1.1",
    "typeorm": "^0.3.19"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "concurrently": "^8.2.2",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-import-resolver-typescript": "^3.5.4",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "nodemon": "^1.14.9",
    "prettier": "^2.8.7",
    "supertest": "^6.1.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsc-watch": "^6.0.0",
    "typescript": "^5.3.3"
  }
}
