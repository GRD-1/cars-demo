{
  "name": "test-project-cars",
  "version": "1.0.0",
  "description": "",
  "module": "app/index.js",
  "type": "module",
  "keywords": [
    "REST",
    "TypeScript",
    "Express",
    "JWT",
    "cli"
  ],
  "author": "GRD",
  "scripts": {
    "start:prod": "npm run build && NODE_ENV=prod node app/index.js",
    "start:dev": "NODE_ENV=dev tsc-watch --onSuccess 'nodemon --config devops/nodemon-dev.json _src/index.ts'",
    "start:debug": "NODE_ENV=debug tsc-watch --onSuccess 'nodemon --config devops/nodemon-debug.json _src/index.ts'",
    "run-typescript": "ts-node-transpile-only",
    "build": "tsc && node app/utils/fix-imports.util.js",
    "test:unit": "jest --config _src/test/unit/jest.unit-config.json",
    "test:int": "jest --config _src/test/integration/jest.integration-config.json",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage --config jest.config.json",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-import-resolver-typescript": "^3.5.4",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "nodemon": "^1.14.9",
    "prettier": "^2.8.7",
    "supertest": "^6.1.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsc-watch": "^6.0.0",
    "typescript": "^5.0.2"
  }
}
